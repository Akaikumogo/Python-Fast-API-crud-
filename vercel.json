{
  "version": 2,
  "builds": [{ "src": "./main.py", "use": "@vercel/python" }],
  "routes": [
    { "src": "/addUser", "dest": "./main.py" },
    { "src": "/Users/:user_id", "dest": "./main.py" },
    { "src": "/AllUsers", "dest": "./main.py" },
    { "src": "/Users/:user_id", "dest": "./main.py" },
    { "src": "/", "dest": "./main.py" },
    { "src": "/Users/:user_id", "dest": "./main.py" }
  ],
  "env": {
    "APP_MODULE": "main:app"
  }
}
